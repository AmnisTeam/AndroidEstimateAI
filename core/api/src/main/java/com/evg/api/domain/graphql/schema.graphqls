schema {
  query: Query
  mutation: Mutation
}

type Query {
  _: Boolean
}

type Mutation {
  registerUser(data: UserRegistration!): UserRegistrationResponse!
  loginUser(data: UserLogin!): UserLoginResponse!
}

input UserRegistration {
  email: String!
  password: String!
}

type UserRegistrationResponse {
  code: Int!
}

input UserLogin {
  email: String!
  password: String!
}

type UserLoginResponse {
  code: Int!
}
